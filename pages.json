{
	 "easycom": {
        "^u-(.*)": "@/uni_modules/uview-ui/components/u-$1/u-$1.vue"
		
    },

	"pages": [
		//pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/tabbar/custom-list-new/custom-list-new",
			"style": {
				"navigationBarTitleText": "定制列表"
			}
		},

		{
			"path": "pages/tabbar/templete-list/templete-list",
			"style": {
				"navigationBarTitleText": "模板选择",
				"navigationStyle": "custom", // 隐藏系统导航栏
				"navigationBarTextStyle": "white" // 状态栏字体为白色，只能为 white-白色，black-黑色 二选一
			}
		},
		{
			"path": "pages/tabbar/template/template",
			"style": {
				"navigationBarTitleText": "内容定制",
				"navigationStyle": "custom" // 隐藏系统导航栏
			}
		},
		{
			"path": "pages/tabbar/template-preview/template-preview",
			"style": {
				"navigationBarTitleText": "定制体验",
				"navigationStyle": "custom", // 隐藏系统导航栏
				"navigationBarTextStyle": "white" // 状态栏字体为白色，只能为 white-白色，black-黑色 二选一
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}